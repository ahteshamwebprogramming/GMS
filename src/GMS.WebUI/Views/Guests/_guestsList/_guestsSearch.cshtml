 @model List<GMS.Infrastructure.ViewModels.Guests.MemberDetailsWithChild>

<div class="formBlock">
    <div style="overflow-y: scroll;height: 140px;">
        <table class="table">
            <thead class="table-dark">
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Unique No</th>
                    <th scope="col">Name</th>
                    <th scope="col">Gender</th>
                    <th scope="col">Phone Number</th>
                    <th scope="col">Email</th>
                </tr>
            </thead>
            <tbody>
                @{
                    if (Model != null)
                    {
                        foreach (var item in Model)
                        {
                            <tr>
                                <td><input type="radio" name="GuestUniqueSelection" onclick="SelectGuestForForm(@item.Id)" /></td>
                                <td>@item.UniqueNo</td>
                                <td>@item.CustomerName</td>
                                <td>@item.GenderName</td>
                                <td>@item.MobileNo</td>
                                <td>@item.Email</td>
                            </tr>
                        }
                    }
                }
            </tbody>
        </table>
    </div>

</div>