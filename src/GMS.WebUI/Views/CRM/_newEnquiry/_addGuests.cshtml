@model GMS.Infrastructure.ViewModels.Guests.GuestsListViewModel
<div class="row">
    <div class="row main-div-bok contact-us-sect1" style="background-color: #f2eae0; margin:0 15px;  padding: 20px;">
        <div class="form-group col-sm-4 col-xs-12 guest-part1" id="AddGuestFormCRM">
            <input type="hidden" asp-for="MemberDetail.Id" dbCol />
            <input type="hidden" asp-for="MemberDetail.GroupId" dbCol />
            <input type="hidden" asp-for="MemberDetail.UHID" dbCol />
            <input type="hidden" asp-for="MemberDetail.UniqueNo" dbCol />
            <input type="hidden" name="MemberDetail.PAXSno" />
            <div id="div_GuestsFormDetailsPartialView">
                @Html.Partial("_newEnquiry/_guestsHotelFormDetails", Model)
                @* AddGuests() *@

            </div>
            <div>
                <input type="button" class="btn btn-warning mt-3" style="float:left;" onclick="AddGuestsEnquiryConfirmation()" value="Save Enquiry" />
                <input type="button" class="btn btn-primary mt-3" style="float:right" onclick="AddGuestsCRM()" value="Book Stay" />
            </div>
        </div>
        <div class="form-group col-sm-8 col-xs-12 guest-part2" style=" background: #6EBCB7; overflow-y: scroll">

            <div class="col-md-12" id="div_GuestsSearchPartial">
                @Html.Partial("_newEnquiry/_guestsSearch", new List<GMS.Infrastructure.ViewModels.Guests.MemberDetailsWithChild>())
            </div>
            <div class="col-md-12" id="div_RatePlanPartialCRM">
                @Html.Partial("_newEnquiry/_ratePlan", Model)
            </div>

        </div>
    </div>
</div>