@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "New Enquiry";
}


<div class="admin_wrapper">
    @Html.Partial("_crm/_leftNavigationCRM")

    <!-- Modal -->
    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Add New Room</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <form class="general_form">
                        <div class="mb-3">
                            <label class="form-label">Room Number</label>
                            <input type="text" class="form-control">
                        </div>
                        <div class="mb-3">
                            <label class="form-label">Room Type</label>
                            <select class="form-control">
                                <option>Room Type</option>
                                <option>Room Type</option>
                                <option>Room Type</option>
                                <option>Room Type</option>
                            </select>
                        </div>
                        <div class="mb-3">
                            <label class="form-label">Room Size</label>
                            <input type="text" class="form-control">
                        </div>
                        <div class="mb-3">
                            <label class="form-label">Bed Type</label>
                            <input type="text" class="form-control">
                        </div>
                        <div class="mb-3">
                            <label class="form-label">Remark</label>
                            <textarea class="form-control" rows="3"></textarea>
                        </div>
                        <div class="mb-3">
                            <label class="form-label">Upload Photos</label>
                            <input type="file" class="form-control">
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary" data-bs-dismiss="modal">Save changes</button>
                </div>
            </div>
        </div>
    </div>
    <div class="right_container">
        <span onclick="openNav()" class="menu_opener"><i class="bi bi-list"></i></span>

        <!-- filtered rooms Start-->
        <div class="filtered_rooms_container">
            <div class="therapiesList_table_list">
                <div class="title_area">
                    <h2>Guest Booking</h2>

                </div>
                <div class="row">
                    <div class="row main-div-bok contact-us-sect1" style="background-color: #f2eae0; margin:0 15px;  padding: 20px;">
                        <div class="form-group col-sm-4 col-xs-12 guest-part1">
                            <div class="row">
                                <div class="col-sm-6 padding0">
                                    <div class="col-xs-12 padding0">

                                        <div class="form-group col-sm-12 col-xs-12">
                                            <label class="control-label">First Name</label>
                                            <input type="text" id="txtfname" name="txtfname" class="contact_name form-control">
                                        </div>
                                        <div class="form-group col-sm-12 col-xs-12">
                                            <label class="control-label">Mid Name</label>
                                            <input type="text" id="txtlname" name="txtlname" class="contact_name form-control">
                                        </div>
                                        <div class="form-group col-sm-12 col-xs-12">
                                            <label class="control-label">Surname</label>
                                            <input type="text" id="txtsurname" name="txtsurname" class="contact_name form-control">
                                        </div>
                                        <div class="form-group col-sm-12 col-xs-12">
                                            <label class="control-label">Brand Awareness</label>
                                            <select id="BrandAwareness" name="BrandAwareness" class="contact_name form-control"><option value="0"> Select </option><option value="1"> Social Media - Facebook </option><option value="2"> Sales/Marketing </option><option value="3"> Search Engine - Google </option><option value="6"> Social Media - Instagram </option><option value="7"> Social Media - Linkedin </option><option value="8"> Plush Escapes </option><option value="9"> Website </option><option value="10"> Word of mouth </option><option value="11"> Promotional Emails </option><option value="12"> Ref. Mr. Khetan </option><option value="14"> Not Mentioned </option></select>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-6 padding0">
                                    <div class="col-xs-12 padding0">
                                        <div class="form-group col-xs-12">
                                            <label class="control-label" style="display: block">Mobile</label>
                                            <input type="text" id="txtemail" name="txtemail" class="contact_name form-control">
                                        </div>
                                        <div class="form-group col-xs-12">
                                            <label class="control-label">Email</label>
                                            <input type="text" id="txtemail" name="txtemail" class="contact_name form-control">
                                        </div>
                                        <div class="form-group col-xs-12">
                                            <label class="control-label">Sale Source</label>
                                            <select name="ddlsource" id="ddlsource" class="contact_name form-control">
                                                <option selected="selected" value="0">--Sources--</option>
                                                <option value="9">Healing Hotels of the World</option>
                                                <option value="8">IN-HOUSE-USE</option>
                                                <option value="7">Naad Wellness</option>
                                                <option value="6">FAB Gateways</option>
                                                <option value="5">MGR</option>

                                            </select>
                                        </div>
                                        <div class="form-group col-xs-12">
                                            <label class="control-label">Lead Source</label>
                                            <select id="LeadSource" name="LeadSource" class="contact_name form-control"><option value="0"> Select </option><option value="3"> Inbound Call </option><option value="4"> Inbound Email </option><option value="5"> Landing Page </option><option value="6"> Reservation Page </option><option value="7"> References </option><option value="8"> Others </option></select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <hr>
                            <div class="row" style="">
                                <div class="col-sm-6 padding0" style="">
                                    <div class="col-xs-12 padding0">
                                        <div class="form-group col-sm-12 col-xs-12">
                                            <label class="control-label">Packages</label>
                                            <select name="ddlpservices" id="ddlpservices" class="contact_name form-control">
                                            </select>
                                        </div>
                                        <div class="form-group col-sm-12 col-xs-12">
                                            <label class="control-label">Additional Nights</label>
                                            <input type="text" id="AddNights" name="AddNights" value="0" onblur="return fixedlength(this, event, 2);" onkeypress="return fixedlength(this, event, 2);" onkeyup="return fixedlength(this, event, 2);" class="contact_name form-control">
                                        </div>
                                        <div class="form-group col-sm-12 col-xs-12">
                                            <label class="control-label">Date of Arrival</label>
                                            <input type="text" id="datepicker" name="datepicker" class="contact_name form-control hasDatepicker">

                                        </div>
                                        <div class="form-group col-sm-12 col-xs-12" style="">
                                            <label class="control-label">PAX</label>
                                            <input type="text" id="txtpax" name="txtpax" onblur="return fixedlength(this, event, 2);" onkeypress="return fixedlength(this, event, 2);" onkeyup="return fixedlength(this, event, 2);" class="contact_name form-control">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6 padding0" style="">
                                    <div class="col-xs-12 padding0">
                                        <div class="form-group col-sm-12 col-xs-12">
                                            <label class="control-label">Number of Nights</label>
                                            <select name="ddlservices" id="ddlservices" class="contact_name form-control">
                                            </select>
                                        </div>
                                        <div class="form-group col-sm-12 col-xs-12">
                                            <label class="control-label">Room Category</label>
                                            <select name="ddlrcate" id="ddlrcate" class="contact_name form-control">
                                            </select>

                                        </div>
                                        <div class="form-group col-sm-12 col-xs-12">
                                            <label class="control-label">Date of Departure</label>
                                            <input type="text" id="deptdate" name="deptdate" class="contact_name form-control">
                                        </div>
                                        <div class="form-group col-sm-12 col-xs-12" style="">
                                            <label class="control-label">No.of Rooms</label>
                                            <input type="text" id="txtroms" name="txtroms" onblur="return fixedlength(this, event, 2);" onkeypress="return fixedlength(this, event, 2);" onkeyup="return fixedlength(this, event, 2);" class="contact_name form-control">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <hr>
                            <div class="col-xs-12">
                                <div class="form-group col-xs-12">
                                    <label class="control-label">Comments</label>
                                    <textarea rows="4" cols="50" id="txtcomm" name="txtcomm" class="contact_name form-control"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="form-group col-sm-8 col-xs-12 guest-part2" style=" background: #6EBCB7; overflow-y: scroll">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- filtered rooms end-->
    @Html.Partial("_bottomNavigation1")

</div>