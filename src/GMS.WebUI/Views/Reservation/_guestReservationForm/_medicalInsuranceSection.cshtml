 @model GMS.Infrastructure.ViewModels.Guests.GuestsListViewModel

<div class="formBlock">
    <div class="col-sm-12 formTitle"><b>MEDICAL INSURANCE</b></div>
    <div class="row">
        <div class="form-group col-sm-4 col-xs-4">
            <label>Name of the Policy Holder</label>
            <input asp-for="MemberDetail.PolicyHolder" type="text" class="contact_name form-control" placeholder="" dbCol>
        </div>
        <div class="form-group col-sm-4 col-xs-4">
            <label>Insurance Company Name</label>
            <input asp-for="MemberDetail.InsuranceCompany" type="text" class="contact_name form-control" placeholder="" dbCol>
        </div>
        <div class="form-group col-sm-4 col-xs-4">
            <label>Policy No </label>
            <input asp-for="MemberDetail.PolicyNo" type="text" class="contact_name form-control" placeholder="" dbCol>
        </div>
        <div class="form-group col-sm-4 col-xs-4">
            <label>Whether it covered in India</label>
            <select asp-for="MemberDetail.CoveredIndia" class="contact_name form-control" dbCol>
                <option value="0">--Whether it covered in India--</option>
                <option value="Yes">Yes</option>
                <option value="No">No</option>
            </select>
        </div>
    </div>
</div>