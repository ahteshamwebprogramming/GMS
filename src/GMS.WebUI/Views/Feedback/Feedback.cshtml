@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, GMSCore
@model GMS.Infrastructure.ViewModels.ReviewAndFeedback.FeedbackViewModel
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Welcome - NAAD Wellness</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/assets/feedback/style.css">
</head>
<body>
    @Html.Partial("_reviewAndFeedback/_header")
    <div class="content-box">
        <h4>Welcome</h4>
        <p>Thank you for becoming a part of NAAD Wellness family</p>
        <p>We value your feedback & suggestions and it will help us immensely in enriching your experience at NAAD.</p>
        <p>We want to know about your experience at NAAD...</p>
        <a href="/ReviewAndFeedbacks/FeedbackLevel2/@Model?.GuestId" class="btn btn-custom mt-4">Next</a>
    </div>
    @Html.Partial("_reviewAndFeedback/_footer")
</body>
</html>
